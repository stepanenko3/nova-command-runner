{
    "name": "stepanenko3/nova-command-runner",
    "description": "Laravel Nova tool for running Artisan and bash(shell) commands.",
    "keywords": [
        "laravel",
        "nova",
        "Artisan",
        "commands",
        "shell",
        "bash"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "laravel/nova": "^4.0",
        "stepanenko3/laravel-helpers": "^1.3.2"
    },
    "autoload": {
        "psr-4": {
            "Stepanenko3\\NovaCommandRunner\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stepanenko3\\NovaCommandRunner\\ToolServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://laravelsatis.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "tightenco/duster": "^2.7"
    }
}
